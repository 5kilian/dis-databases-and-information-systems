<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="400.0"
      prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.112"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.estate.manager.controller.HomeController">
  <fx:include source="menu.fxml" />

  <Pane VBox.vgrow="SOMETIMES" />

  <HBox>
    <Pane HBox.hgrow="SOMETIMES" />
    <Button text="Agent"
            fx:id="agentButton"
            onAction="#openAgentManager" />
    <Pane HBox.hgrow="SOMETIMES" />
    <Button text="Estate"
            fx:id="estateButton"
            onAction="#openEstateManager" />
    <Pane HBox.hgrow="SOMETIMES" />
    <Button text="Contract"
            fx:id="contractButton"
            onAction="#openContractManager" />
    <Pane HBox.hgrow="SOMETIMES" />
  </HBox>

  <Pane VBox.vgrow="SOMETIMES" />

</VBox>
